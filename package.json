{
  "name": "test-server",
  "version": "1.0.0",
  "description": "EnDronist's NodeJS Server",
  "main": "index.js",
  "author": "EnDronist",
  "license": "MIT",
  "scripts": {
    "build": "cd client && webpack --config webpack.config.cjs --mode production",
    "client": "cd client && webpack-dev-server --config webpack.config.cjs --mode development",
    "dev-server": "cd server && ts-node -r tsconfig-paths/register Server.ts --port 3000 with_errors",
    "server": "cd server && ts-node -r tsconfig-paths/register Server.ts --port 3000",
    "build-server": "npm run build && npm run server"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "connected-react-router": "^6.7.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.4.2",
    "express": "^4.17.3",
    "express-async-handler": "^1.1.4",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.0",
    "html-webpack-plugin": "^3.2.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "promise-mysql": "^4.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7"
  }
}
